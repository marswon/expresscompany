<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zhbit.expresscompany.domain.USenders" table="USenders">
		<id name="usid"/>	
		<property name="uid" type="string" length="32"/>
		<property name="usname" type="string" length="16"/>
		<property name="usphone" type="string" length="16"/>
		<property name="usaddress" type="string" length="128"/>	
	</class>	
</hibernate-mapping>
